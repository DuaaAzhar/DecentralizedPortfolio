{
  "name": "decentralized-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy:sepolia": "hardhat run scripts/deploy.cjs --network sepolia",
    "deploy:mumbai": "hardhat run scripts/deploy.cjs --network mumbai",
    "deploy:localhost": "hardhat run scripts/deploy.cjs --network localhost",
    "node": "hardhat node",
    "verify:sepolia": "hardhat verify --network sepolia",
    "interact": "hardhat run scripts/interact.cjs --network localhost",
    "setup": "node scripts/setup-env.js",
    "setup:dev": "npm run compile && npm run node"
  },
  "dependencies": {
    "@metamask/detect-provider": "^2.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "aos": "^2.3.4",
    "axios": "^1.6.0",
    "ethers": "^6.15.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.544.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.11.0",
    "react-intersection-observer": "^9.5.2",
    "react-router-dom": "^6.17.0",
    "web-vitals": "^2.1.4",
    "web3": "^4.2.2"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@openzeppelin/contracts": "^5.0.0",
    "@openzeppelin/test-helpers": "^0.5.16",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "hardhat": "^2.19.0",
    "hardhat-gas-reporter": "^1.0.9",
    "postcss": "^8.4.47",
    "solidity-coverage": "^0.8.5",
    "tailwindcss": "^3.4.14",
    "vite": "^5.2.0"
  }
}
